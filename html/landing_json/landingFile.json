u0432 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u0438 \u0434\u043e\u0445\u043e\u0434\u0430 \u043d\u0430 \u0441\u0443\u043c\u043c\u0443 580 \u0440\u0443\u0431\u043b\u0435\u0439 \u0432 \u0434\u0435\u043d\u044c. \u0421 \u044d\u0442\u043e\u0439 \u0442\u043e \u0432\u0435\u043b\u0438\u043a\u043e\u0439 \u043d\u043e\u0442\u044b \u043d\u0430\u0434\u043e \u043d\u0430\u0447\u0438\u043d\u0430\u0442\u044c \u0441\u0432\u043e\u0439 \u043f\u0443\u0442\u044c \u0442\u0430\u043a\u043e\u043c \u0442\u044f\u0436\u0435\u043b\u043e\u043c \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0435. \u0418 \u0443\u0447\u0438\u0442\u044b\u0432\u0430\u0442\u044c \u0447\u0442\u043e 580 \u0440\u0443\u0431\u043b\u0435\u0439 \u044d\u0442\u043e \u043c\u0438\u043d\u0438\u043c\u0430\u043b\u044c\u043d\u0430\u044f \u0446\u0435\u043d\u0430, \u043d\u043e \u043d\u0438\u043a\u0430\u043a \u043d\u0435 \u043f\u0440\u0438\u0434\u0435\u043b.\r\n\u0415\u0441\u0442\u044c \u043e\u0434\u043d\u043e \u0441\u043b\u043e\u0432\u043e\u0441\u043e\u0447\u0435\u0442\u0430\u043d\u0438\u0435, \u043a\u043e\u0442\u043e\u0440\u043e\u0435 \u0437\u0432\u0443\u0447\u0438\u0442 \u043f\u043e \u0433\u0435\u0440\u043e\u0439\u0441\u043a\u0438: \u201c\u041d\u0438\u043a\u0442\u043e \u043a\u0440\u043e\u043c\u0435 \u041d\u0430\u0441, \u0432 \u044d\u0442\u043e\u0442 \u043a\u043e\u0434 \u043d\u0435 \u043f\u043e\u043b\u0435\u0437\u0435\u0442\u201d.\r\n\u041b\u044e\u0434\u0438 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u0432 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u0438 \u043e\u043f\u043b\u0430\u0442\u044b, \u043f\u043e \u0442\u0435\u043c \u0436\u0435 \u043f\u0440\u0438\u043d\u0438\u0446\u0438\u043f\u0430\u043c \u0447\u0442\u043e \u0432\u0435\u0441\u044c \u0440\u0430\u0431\u043e\u0447\u0438\u0439 \u043a\u043b\u0430\u0441\u0441 <a href=\u201dhttps:\/\/www.google.com\/search?q=%D0%BF%D1%80%D0%BE%D0%B6%D0%B8%D1%82%D0%BE%D1%87%D0%BD%D1%8B%D0%B9+%D0%BC%D0%B8%D0%BD%D0%B8%D0%BC%D1%83%D0%BC&sxsrf=ALeKk01nnxODGME2qzZwCTfLID1bTxuPOg:1626829653714&source=lnms&tbm=isch&sa=X&sqi=2&ved=2ahUKEwiCtffc_PLxAhVywIsKHe7DDwIQ_AUoAnoECAEQBA&biw=1848&bih=949#imgrc=0bHFnLGWd2fT3M&imgdii=TNKOAouaY-4rpM\u201d>\u043c\u0438\u043d\u0438\u043c\u0430\u043b\u043a\u0430<\/a> \r\n\u0421\u0442\u043e\u0438\u0442\u044c \u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c \u043d\u0430 \u043e\u0431\u044b\u0447\u043d\u044b\u0435 \u0432\u0435\u0449\u0438 \u0442\u0440\u0435\u0437\u0432\u043e, \u0431\u0435\u0437 \u043f\u0440\u0438\u0432\u0440\u0430\u0442\u043d\u043e\u0439 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0438 \u0432\u044b\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0439 \u043d\u0430 \u043f\u043e\u043a\u0430\u0437: \u201c\u0447\u0442\u043e \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u044b, \u0442\u043e \u0443 \u0433\u0440\u0430\u0436\u0434\u0430\u043d 200 \u0442\u044b\u0441\u044f\u0447\u201d. \u0422\u043e\u043b\u044c\u043a\u043e \u043a\u0430\u043a \u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u0434\u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043b\u043e\u0433\u0438\u043a\u0430: \u043a\u0442\u043e-\u0442\u043e, \u0447\u0442\u043e-\u0442\u043e \u201c\u0437\u0430\u043a\u0440\u044b\u0441\u0438\u043b\u201d \u0438 \u043d\u0430\u0433\u043b\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u0441\u043a\u043e\u043c\u043f\u0440\u043e\u043c\u0435\u0442\u0438\u0440\u043e\u0432\u0430\u043b \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0443. \u0418 \u0432\u044b\u0445\u043e\u0434\u0438\u0442 \u0447\u0442\u043e \u043a\u0440\u044b\u0441\u0430 \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442 2 000 000, \u0430 \u0442\u0440\u0443\u0434\u044f\u0433\u0430 20 000. \r\n<a href=\u201dhttps:\/\/trudvsem.ru\/\u201d><picture>\r\n<img src=\u201d#\u201d alt=\u201d\u043e\u043f\u043b\u0430\u0442\u0430 \u0442\u0440\u0443\u0434\u0430\u201d>\r\n<\/picture><\/a>\r\n\u041f\u043e \u044d\u0442\u043e\u0439 \u0441\u0445\u0435\u043c\u0435 \u0438 \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u201c\u043f\u043e\u0434\u043f\u043e\u043b\u044c\u043d\u043e \u043a\u043e\u043c\u043c\u0443\u043d\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f\u201d \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0430. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043a\u0430\u043a \u0447\u0435\u0441\u0442\u043d\u043e\u043c\u0443 \u0438 \u0437\u0430\u043a\u043e\u043d\u043e\u043f\u043e\u0441\u043b\u0443\u0448\u043d\u043e\u043c\u0443 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0438\u043d\u0443, \u043f\u0440\u044f\u043c \u043a\u0430\u043a \u0432 \u0444\u0438\u043b\u044c\u043c\u0435: \u201c\u0437\u0430\u043a\u043e\u043d\u043e\u043f\u043e\u0441\u043b\u0443\u0448\u043d\u044b\u0439 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0438\u043d\u201d. \u041d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u043e\u0441\u0442\u0430\u0435\u0442\u0441\u044f \u043a\u0430\u043a \u043f\u043e\u0434\u0443\u043c\u0430\u0442\u044c \u043e \u043b\u044e\u0434\u044f\u0445 \u0438 \u043e\u043f\u0443\u0441\u0442\u0438\u0442\u044c \u0446\u0435\u043d\u043d\u0438\u043a \u043d\u0430 \u0432\u0435\u0440\u0441\u0442\u043a\u0443 \u043b\u0435\u043d\u0434\u0438\u043d\u0433\u0430, \u0441 SEO \u043e\u043f\u0442\u0438\u043c\u0438\u0437\u0430\u0446\u0438\u0435\u0439 \u0438 \u043f\u0440\u043e\u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435\u043c \u0432 \u043a\u043e\u043d\u0442\u0435\u043a\u0441\u0442\u043d\u043e\u0439 \u0440\u0435\u043a\u043b\u0430\u043c\u0435(\u0442\u0443\u0440\u0431\u043e \u043f\u0440\u0438\u0432\u043e\u0434\u043e\u043c \u0432 \u0442\u043e\u043f!).\r\n\u0418 \u0434\u043b\u044f \u043f\u0443\u0449\u0435\u0433\u043e \u0443\u0434\u0435\u0448\u0435\u0432\u043b\u0435\u043d\u0438\u044f \u0435\u0441\u043b\u0438 \u0444\u0438\u043d\u0430\u043d\u0441\u044b \u0441\u043e\u0432\u0441\u0435\u043c \u043f\u043e\u044e\u0442 \u0440\u043e\u043c\u0430\u043d\u0441\u044b, \u0442\u043e \u0448\u0438\u043a\u0430\u0440\u043d\u044b\u043c \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u043e\u043c \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u043e\u0434\u0443\u043c\u0430\u0442\u044c \u043e \u0431\u043e\u043b\u0435\u0435 \u0443\u0441\u043f\u0435\u0448\u043d\u043e\u0439 \u043d\u0430\u0441\u0442\u0440\u043e\u0439\u043a\u0435 \u043a\u043e\u043d\u0442\u0435\u043a\u0441\u0442\u043d\u043e\u0439 \u0440\u0435\u043a\u043b\u0430\u043c\u044b \u0438 \u0437\u0430\u043f\u0438\u043b\u0438\u0432\u0430\u0442\u044c \u043b\u0435\u043d\u0434\u0438\u043d\u0433 \u043d\u0430 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e\u043c \u0445\u043e\u0441\u0442\u0438\u043d\u0433\u0435. \u0418\u043b\u0438 \u043d\u0430 \u0433\u0438\u0442\u0445\u0430\u0431\u0435! \u0412 \u0442\u0435\u043e\u0440\u0438\u0438 \u043a \u0433\u0438\u0442\u0445\u0430\u0431\u0443 \u043d\u0430 \u0432\u0441\u044f\u043a\u0438\u0439 \u0441\u043b\u0443\u0447\u0430\u0439 \u043c\u043e\u0436\u043d\u043e \u0435\u0449\u0435 \u0438 \u043f\u0440\u0438\u043f\u0430\u0440\u043a\u043e\u0432\u0430\u0442\u044c \u0434\u043e\u043c\u0435\u043d. \u0422\u043e\u0433\u0434\u0430 \u0441\u0435\u043e \u043e\u043f\u0442\u0438\u043c\u0438\u0437\u0430\u0446\u0438\u044e \u043d\u0443\u0436\u043d\u043e \u043f\u0440\u043e\u0434\u0435\u043b\u044b\u0432\u0430\u0442\u044c, \u0442\u043e\u043c\u0443 \u0434\u043e\u043c\u0435\u043d\u0443. \u0422.\u043a. \u0434\u043e\u043c\u0435\u043d \u0432 \u044d\u0442\u043e\u043c \u0441\u043b\u0443\u0447\u0430\u0435 \u0443\u0436\u0435 \u0431\u0443\u0434\u0435\u0442 \u043b\u0438\u0446\u043e\u043c \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438. \u0422\u0430\u043a\u043e\u043c\u0443 \u043b\u0438\u0446\u0443, \u043d\u0435\u043b\u044c\u0437\u044f \u043f\u0440\u043e\u0434\u0432\u0438\u0433\u0430\u0442\u044c \u0434\u043e\u043c\u0435\u043d \u043f\u043e \u043e\u0434\u043d\u0438\u043c \u043a\u043b\u044e\u0447\u0435\u0432\u0438\u043a\u0430\u043c, \u0430 \u043f\u043e\u0442\u043e\u043c \u0440\u0435\u0437\u043a\u043e \u043e\u0442\u043e\u0431\u0440\u0430\u0442\u044c \u0435\u0433\u043e \u0443\u0433\u0440\u043e\u0436\u0430\u044f \u043b\u0438\u0445\u0438\u043c \u0437\u0430\u043a\u043e\u043d\u043e\u043c \u201c\u0441 \u043a\u043e\u043c\u043c\u0443\u043d\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u0440\u0430\u0441\u043f\u0440\u0430\u0432\u0430\u043c\u0438\u201d. \u0421 \u0443\u0432\u0435\u0440\u0435\u043d\u043d\u043e\u0441\u0442\u044c\u044e \u043c\u043e\u0436\u043d\u043e \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u0447\u0442\u043e \u043d\u0438\u0447\u0435\u0433\u043e \u0445\u043e\u0440\u043e\u0448\u0435\u0433\u043e \u0438\u0437 \u044d\u0442\u043e\u0433\u043e \u043d\u0435 \u0432\u044b\u0439\u0434\u0435\u0442.\r\n\u041f\u043e \u0442\u0430\u043a\u043e\u043c\u0443 \u0441\u043b\u0443\u0447\u0430\u044e \u043e\u043f\u0438\u0441\u0430\u043d\u043e \u0432 \u0432\u0435\u0434\u0430\u0445: \u0416\u0438\u0432\u043e\u0442\u043d\u044b\u0435 \u0434\u043e\u0441\u0442\u0438\u0433\u0430\u044f \u0440\u0430\u0437\u0443\u043c\u0435\u043d\u0438\u044f, \u043f\u0435\u0440\u0435\u0440\u043e\u0436\u0434\u0430\u044e\u0442\u0441\u044f \u0432 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430. \u041d\u043e \u0438 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u044d\u0442\u043e \u043d\u0435 \u0441\u0430\u043c\u0430\u044f \u0432\u044b\u0441\u0448\u0430\u044f \u0441\u0442\u0443\u043f\u0435\u043d\u044c. \u041a\u0430\u043a \u0437\u0430\u043c\u0435\u0442\u0438\u043b\u0438 \u0443\u0447\u0435\u043d\u044b\u0435, \u0442\u043e \u043c\u044b\u0441\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0438 \u0443\u043c\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u043e\u0441\u0442\u0438 \u0443 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0436\u0438\u0432\u043e\u0442\u043d\u044b\u0445 \u043f\u0440\u0435\u0432\u043e\u0441\u0445\u043e\u0434\u044f\u0442 \u0447\u0435\u043b\u043e\u0432\u0435\u0447\u0435\u0441\u043a\u0438\u0435. \u201c\u0416\u0430\u043a \u0424\u0440\u0435\u0441\u043a\u043e\u201d \u0433\u043e\u0432\u043e\u0440\u0438\u043b \u0432 \u043e\u0434\u043d\u043e\u0439 \u0438\u0437 \u0441\u0432\u043e\u0438\u0445 \u043f\u0435\u0440\u0435\u0434\u0430\u0447\u0435\u0439. \u041d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u043e\u0441\u0442\u0440\u044b\u0439 \u0432\u043e\u043f\u0440\u043e\u0441 \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432\u0441\u0442\u0430\u0435\u0442 \u043f\u0435\u0440\u0435\u0434 \u043b\u044e\u0434\u044c\u043c\u0438 \u043f\u043e\u0448\u0435\u0434\u0448\u0438\u0445 \u043d\u0430 \u043f\u043e\u0438\u0441\u043a\u0438 \u0432\u043d\u0435\u0437\u0435\u043c\u043d\u043e\u0433\u043e \u0440\u0430\u0437\u0443\u043c\u0430: \u201c\u0410 \u0435\u0441\u0442\u044c \u043b\u0438 \u0440\u0430\u0437\u0443\u043c\u043d\u0430\u044f \u0436\u0438\u0437\u043d\u044c \u043d\u0430 \u0417\u0435\u043c\u043b\u0435?\u201d.\r\n\u042d\u0442\u043e \u043e\u0447\u0435\u043d\u044c \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u0432\u043e\u043f\u0440\u043e\u0441 \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0442\u0435\u0440\u0437\u0430\u0435\u0442 \u043f\u044b\u0442\u043b\u0438\u0432\u044b\u0435 \u0443\u043c\u044b.\r\n\u0412\u0435\u043b\u0438\u043a\u0438\u0435 \u043b\u044e\u0434\u0438 , \u043e\u0442\u0432\u043e\u0435\u0432\u0430\u043b\u0438 \u0441\u0432\u043e\u0431\u043e\u0434\u0443 \u0432 \u0431\u0435\u0437\u0443\u043c\u043d\u043e\u0439 \u0432\u043e\u0439\u043d\u0435, \u0441 \u0433\u043e\u043b\u043e\u0434\u043e\u043c, \u0445\u043e\u043b\u043e\u0434\u043e\u043c \u0438 \u0432\u0441\u0435\u043c \u0441\u043e\u043f\u0443\u0442\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u043c \u0434\u0438\u0441\u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043e\u043c. \u0413\u043e\u0432\u043e\u0440\u044f: \u201c\u043d\u0438\u043a\u0430\u043a\u0430\u044f \u0440\u0430\u0441\u043e\u0432\u0430\u044f \u0434\u0438\u0441\u043a\u0440\u0438\u043c\u0438\u043d\u0430\u0446\u0438\u044f \u043d\u0435 \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u0430 \u0434\u0438\u0441\u043a\u0440\u0438\u043c\u0438\u043d\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u043e\u0433\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430, \u0434\u0430\u0436\u0435 \u0435\u0441\u043b\u0438 \u0442\u044b \u043c\u0410\u0430\u0441\u043a\u043e\u0432\u0441\u043a\u0430\u044f \u043a\u0440\u044b\u0441\u0430\u201d. \u0417\u0430\u043a\u043e\u043d \u0434\u043b\u044f \u0432\u0441\u0435\u0445 \u043e\u0434\u0438\u043d\u0430\u043a\u043e\u0432. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043d\u0430 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043f\u0440\u043e\u0441\u0442\u043e \u043d\u0435 \u0441\u0442\u043e\u0438\u0442 \u043e\u0431\u0440\u0430\u0449\u0430\u0442\u044c \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f. \u0415\u0441\u043b\u0438 \u043d\u0430 \u0442\u0435\u0431\u044f \u043b\u0430\u0435\u0442 \u0441\u043e\u0431\u0430\u043a\u0430, \u0442\u044b \u0436\u0435 \u043d\u0435 \u043f\u043e\u0439\u0434\u0435\u0448\u044c \u0432 \u0441\u0443\u0434 \u043f\u043e\u0434\u0430\u0432\u0430\u0442\u044c \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0435 \u0438 \u0441\u0443\u0434\u0438\u0442\u044c\u0441\u044f \u0441 \u0431\u0435\u0448\u0435\u043d\u044b\u043c \u043f\u0441\u043e\u043c, \u0441\u0438\u0434\u044f\u0449\u0435\u043c \u043d\u0430 \u0446\u0435\u043f\u0438. \u0414\u0430\u0436\u0435 \u0435\u0441\u043b\u0438 \u043f\u043e \u201c\u043d\u0435\u043e\u0431\u044a\u044f\u0441\u043d\u0438\u043c\u044b\u043c \u043d\u043e \u0444\u0430\u043a\u0442\u043e\u0432\u044b\u043c \u043f\u0440\u0438\u0447\u0438\u043d\u0430\u043c\u201d. \u201c\u0421\u043b\u0443\u0447\u0430\u0439\u043d\u043e\u0441\u0442\u044c \u0437\u0430\u0441\u0442\u0430\u0432\u0438\u0442\u201d, \u0441\u0445\u0432\u0430\u0442\u0438\u0442\u044c\u0441\u044f \u0432 \u0431\u0435\u0437\u0443\u043c\u043d\u043e\u043c \u0431\u043e\u044e \u0441 \u0431\u0435\u0448\u0435\u043d\u044b\u043c \u0440\u044b\u0447\u0430\u043d\u0438\u0435\u043c. \u0423\u0434\u0430\u0441\u0442\u0441\u044f \u043f\u043e\u0434\u0430\u0442\u044c \u0432 \u0441\u0443\u0434 \u043d\u0430 \u0446\u0435\u043f\u043d\u043e\u0433\u043e \u043f\u0441\u0430, \u0430 \u0447\u0442\u043e \u0434\u0430\u043b\u044c\u0448\u0435? \u0412\u043e\u0442 \u0412\u044b \u0443\u0432\u0430\u0436\u0430\u0435\u043c\u044b\u0439 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u0436 \u043f\u043e\u0434\u0430\u043b\u0438 \u0432 \u0441\u0443\u0434 \u043d\u0430 \u0435\u0449\u0435 \u0432 \u043f\u0440\u043e\u0448\u043b\u043e\u0439 \u0436\u0438\u0437\u043d\u0438 \u0431\u0435\u0448\u0435\u043d\u043e\u0433\u043e \u043f\u0441\u0430, \u0432\u044b\u0438\u0433\u0440\u0430\u043b\u0438 \u0434\u0435\u043b\u043e, \u043e\u0442\u0441\u0443\u0434\u0438\u043b\u0438 \u0443 \u043d\u0435\u0433\u043e \u043a\u043e\u0441\u0442\u044c. \u0414\u0430\u043b\u044c\u0448\u0435 \u0442\u043e \u0447\u0442\u043e? \u0414\u0430\u043b\u044c\u0448\u0435 \u0432\u043f\u043e\u043b\u043d\u0435 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u0447\u0442\u043e \u0432 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u0436\u0438\u0437\u043d\u0438 \u0432 \u0437\u043d\u0430\u043a \u0431\u0435\u0441\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u0445 \u043f\u043e\u0431\u0435\u0434, \u0412\u044b \u0443\u0432\u0430\u0436\u0430\u0435\u043c\u044b\u0439 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u0436, \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043f\u043e\u043e\u0449\u0440\u0435\u043d\u0438\u044f \u0438 \u043d\u0430\u0433\u0440\u0430\u0434\u044b \u0437\u0430 \u201c\u044d\u043f\u0438\u0447\u0435\u0441\u043a\u0443\u044e \u0431\u0438\u0442\u0432\u0443\u201d, \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u0437\u0430\u043a\u043e\u043d\u0430. \u0421\u043c\u043e\u0436\u0435\u0442\u0435 \u0441 \u0447\u0438\u0441\u0442\u043e\u0439 \u0441\u043e\u0432\u0435\u0441\u0442\u044c\u044e \u0437\u0430\u043d\u044f\u0442\u044c \u0435\u0433\u043e \u043c\u0435\u0441\u0442\u043e.\r\n","very_cheap"],["35","\u0412 \u043f\u043e\u0437\u0438\u0442\u0438\u0432\u043d\u043e\u043c \u043a\u043b\u044e\u0447\u0435!","\u0415\u0441\u043b\u0438 \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u0442\u044c \u0441\u0432\u043e\u0439 \u0440\u0430\u0431\u043e\u0447\u0438\u0439 \u043f\u0440\u043e\u0446\u0435\u0441\u0441. \u0427\u0442\u043e \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0432\u0430\u0436\u043d\u0430\u044f \u0438 \u0432\u043e\u0441\u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u043d\u0430\u044f \u0440\u0430\u0431\u043e\u0442\u0430 \u043f\u0440\u043e\u0442\u0435\u043a\u0430\u0435\u0442 \u0432 \u201c\u043f\u043e\u0437\u0438\u0442\u0438\u0432\u043d\u043e\u043c \u043a\u043b\u044e\u0447\u0435\u201d. \u0423 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u0436\u0435\u0439 \u0441\u0442\u0430\u0440\u043e\u0439 \u0437\u0430\u043a\u0430\u043b\u043a\u0438 \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u0435\u0442 \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0447\u0442\u043e \u044d\u0442\u043e \u0432\u0441\u0435 \u044e\u043c\u043e\u0440. ","\u041d\u043e \u043a \u0412\u0430\u0448\u0435\u0439 \u0440\u0430\u0434\u043e\u0441\u0442\u0438 \u0438\u043b\u0438 \u0440\u0430\u0437 \u201c\u0441\u0442\u0430\u0440\u043e\u0439 \u0437\u0430\u043a\u0430\u043b\u043a\u0438\u201d, \u0435\u0449\u0435 \u043a\u0430\u043a\u0438\u0445 \u043b\u0438\u0431\u043e \u0431\u043e\u043b\u0435\u0435 \u0441\u043c\u0443\u0442\u043d\u044b\u0445 \u0434\u043e\u0433\u0430\u0434\u043e\u043a. \u0414\u0435\u043b\u043e \u0432 \u201c\u043f\u043e\u0437\u0438\u0442\u0438\u0432\u043d\u043e\u043c \u043a\u043b\u044e\u0447\u0435\u201d - \u044d\u0442\u043e \u0441\u0435\u0440\u044c\u0435\u0437\u043d\u0430\u044f \u0442\u0435\u0445\u043d\u0438\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0432\u0435\u0434\u0435\u0442 \u043a \u0443\u0441\u043f\u0435\u0445\u0443! \u041c\u043d\u043e\u0433\u0438\u043c \u043b\u044e\u0434\u044f\u043c \u043f\u0440\u0438\u0432\u044b\u043a\u0448\u0438\u043c \u043a \u043d\u0435\u0441\u043f\u0440\u0430\u0432\u0435\u0434\u043b\u0438\u0432\u043e\u043c\u0443 \u0431\u0430\u0431\u0443\u0438\u043d\u0443 \u0443\u0433\u0440\u0